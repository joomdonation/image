{
    "name": "joomla/image",
    "type": "joomla-package",
    "description": "Joomla Image Package",
    "keywords": ["joomla", "framework", "image"],
    "homepage": "https://github.com/joomla-framework/image",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "~7.0|~8.0",
        "ext-gd": "*",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "joomla/coding-standards": "~2.0@alpha",
        "joomla/test": "~1.0",
        "phpunit/phpunit": "~6.3"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Image\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Image\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
	        "dev-2.0-dev": "2.0-dev"
        }
    }
}
